<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    {% block title %}
        <title>Dashboard</title>
    {% endblock %}


    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}" />

    <link rel="stylesheet" href="{{ asset('css/sb-admin-2.css') }}" />
    <link rel="stylesheet" href="{{ asset('css/metisMenu.min.css') }}" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>


<div id="wrapper">

    {{ render(controller('AdminBundle:Default:menu', {'id_clan' : id_clan  })) }}

    <div id="page-wrapper">

        {% block body %}{% endblock %}
        <!-- /.row -->
    </div>
    <!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->



<script src="{{ asset('js/jquery.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/jquery.swipebox.min.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/sb-admin-2.js') }}" type="text/javascript"></script>
<script src="{{ asset('js/metisMenu.min.js') }}" type="text/javascript"></script>
<!-- bootbox code -->
<script src="{{ asset('js/bootbox.min.js') }}"></script>
<script>

    $(document).on("click", ".alert", function(e) {
        var link = $(this).attr("href"); // "get" the intended link in a var
        e.preventDefault();
        bootbox.confirm("Are you sure?", function(result) {
            if (result) {
                document.location.href = link;  // if result, "set" the document location
            }
        });
    });

</script>

{% block javascripts %}{% endblock %}


</body>
</html>

