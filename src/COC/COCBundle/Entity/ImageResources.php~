<?php

namespace COC\COCBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\HttpFoundation\File\UploadedFile;
use Sensio\Bundle\FrameworkExtraBundle\Configuration\Template;
use Symfony\Component\HttpFoundation\Request;
use Symfony\Component\Validator\Constraints as Assert;
use COC\COCBundle\Entity\Image as BaseImage;
/**
 * Image
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="COC\COCBundle\Repository\ImageResourcesRepository")
 */
class ImageResources extends BaseImage
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="gold", type="integer")
     */
    private $gold;

    /**
     * @var integer
     *
     * @ORM\Column(name="elixir", type="integer")
     */
    private $elixir;



    /**
     * @var string
     */
    public $name;

    /**
     * @var string
     */
    public $path;

    /**
     * @var \User\UserBundle\Entity\User
     */
    public $user;

    /**
     * @var \COC\COCBundle\Entity\CategoryImage
     */
public $categoryImage;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set gold
     *
     * @param integer $gold
     * @return ImageResources
     */
    public function setGold($gold)
    {
        $this->gold = $gold;

        return $this;
    }

    /**
     * Get gold
     *
     * @return integer 
     */
    public function getGold()
    {
        return $this->gold;
    }

    /**
     * Set elixir
     *
     * @param integer $elixir
     * @return ImageResources
     */
    public function setElixir($elixir)
    {
        $this->elixir = $elixir;

        return $this;
    }

    /**
     * Get elixir
     *
     * @return integer 
     */
    public function getElixir()
    {
        return $this->elixir;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return ImageResources
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set path
     *
     * @param string $path
     * @return ImageResources
     */
    public function setPath($path)
    {
        $this->path = $path;

        return $this;
    }

    /**
     * Get path
     *
     * @return string 
     */
    public function getPath()
    {
        return $this->path;
    }

    /**
     * Set user
     *
     * @param \User\UserBundle\Entity\User $user
     * @return ImageResources
     */
    public function setUser(\User\UserBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \User\UserBundle\Entity\User 
     */
    public function getUser()
    {
        return $this->user;
    }

    /**
     * Set categoryImage
     *
     * @param \COC\COCBundle\Entity\CategoryImage $categoryImage
     * @return ImageResources
     */
    public function setCategoryImage(\COC\COCBundle\Entity\CategoryImage $categoryImage = null)
    {
        $this->categoryImage = $categoryImage;

        return $this;
    }

    /**
     * Get categoryImage
     *
     * @return \COC\COCBundle\Entity\CategoryImage 
     */
    public function getCategoryImage()
    {
        return $this->categoryImage;
    }
}
