{% extends "::base_admin.html.twig" %}
{% set id_clan = clan.id %}

{% block body %}


    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{ 'clan.parameters'|trans }}</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>


    <ol class="breadcrumb">
        <li><a href="{{ path('admin', { 'id_clan': clan.id }) }}"><p class="fa fa-home"></p> Dashboard</a></li>
        <li class="active">{{ 'menu.edit_clan'|trans }}</li>
    </ol>



    <div class="well">
        {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
        {{ form_errors(form) }}

        <div class="form-group">
            <div class="col-sm-12 text-center">
                {{ form_widget(form.save, {'attr': {'class': 'btn btn-standard btn-primary'}}) }}
            </div>
        </div>
        {{ form_rest(form) }}
        {{ form_end(form) }}
        {{ form(form)}}
    </div>

{% endblock %}

